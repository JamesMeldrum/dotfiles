## FORTUNE

if [ `which fortune` ]; then
  echo ""
  fortune
fi

## RBENV
if [ `which rbenv` -a $HTMLDRUM_RBENV ]
  then
    eval "$(rbenv init -)"
  else
    echo "no rbenv $HTMLDRUM_RBENV"
fi

## NVM
[[ -r $NVM_DIR/bash_completion ]] && . $NVM_DIR/bash_completion
if [ -e ~/.nvm/nvm.sh ]; then
  source ~/.nvm/nvm.sh ### NVM
fi
